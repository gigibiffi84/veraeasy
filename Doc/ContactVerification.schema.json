{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "mobileNumber": {
      "type": "string",
      "pattern": "^([+]39)?((38[{8,9}|0])|(34[{7-9}|0])|(36[6|8|0])|(33[{3-9}|0])|(32[{8,9}]))([\\d]{7})$"
    },
    "businessKey": {
      "type": "string"
    },
    "personId": {
      "type": "string",
      "pattern": "^[A-Z]{6}[A-Z0-9]{2}[A-Z][A-Z0-9]{2}[A-Z][A-Z0-9]{3}[A-Z]$"
    },
    "personIdStrategy": {
      "type": "string",
      "enum": [
        "IT_SSN",
        "EN_SSN"
      ]
    }
  },
  "required": [
    "id",
    "email",
    "mobileNumber",
    "businessKey",
    "personId",
    "personIdStrategy"
  ]
}